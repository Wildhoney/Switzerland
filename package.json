{
    "name": "switzerland",
    "version": "5.8.0",
    "description": "Switzerland takes a functional approach to Web Components by applying middleware to your components. Supports Redux, attribute mutations, CSS variables, React-esque setState/state, etcâ€¦ out-of-the-box, along with Shadow DOM for style encapsulation and Custom Elements for interoperability.",
    "type": "module",
    "main": "./src/index.js",
    "module": "./es/index.js",
    "repository": "git@github.com:Wildhoney/Switzerland.git",
    "author": "Adam Timberlake <adam.timberlake@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=14.x"
    },
    "scripts": {
        "build": "rollup -c rollup.config.js",
        "start": "ln -nsf ../ node_modules/switzerland && nodemon ./example/server/index.mjs",
        "format": "prettier --write '{src,tests,example}/**/*.{mjs,js,css,html,yml,json,md}'",
        "test": "yarn lint && yarn spec",
        "spec": "ava",
        "coverage": "nyc ava",
        "prepublishOnly": "yarn format && yarn test && yarn build",
        "lint": "eslint '{src,tests,example}/**/**.js' && yarn fslint",
        "fslint": "yarn build && (cd es/ && tar -zcf ../build.tar.gz index.js) && (fslint --files=build.tar.gz --limit=6144 || rm -rf build.tar.gz es/) && rm -rf build.tar.gz es/"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format && yarn test",
            "pre-push": "yarn format && yarn test"
        }
    },
    "dependencies": {
        "morphdom": "^2.6.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "ava": "^3.15.0",
        "babel-eslint": "^10.1.0",
        "browser-env": "^3.3.0",
        "capitalize": "^2.0.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "coveralls": "^3.1.0",
        "delay": "^4.4.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-standard": "^5.0.0",
        "express": "^4.17.3",
        "fslint": "^1.1.0",
        "glob": "^7.1.6",
        "husky": "^4.3.8",
        "jsdom": "^16.4.0",
        "mkdirp": "^1.0.4",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "ramda": "^0.27.1",
        "rollup": "^2.36.2",
        "rollup-plugin-terser": "^7.0.2",
        "sinon": "^9.2.3",
        "starwars": "^1.0.1",
        "string-template": "^1.0.0"
    }
}
